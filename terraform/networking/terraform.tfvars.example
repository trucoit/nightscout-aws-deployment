# Example Terraform variables file
# Copy this file to terraform.tfvars and modify the values as needed

# Prefix name for all the auto generated resources
resources_prefix_name = "auto-networking"

# Specifies the number of days you want to retain log events
# Valid values: 1, 3, 5, 7, 14, 30, 60, 90, 120, 150, 180, 365, 400, 545, 731, 1827, 3653
retention_in_days = 14

# The type of traffic to log
# Valid values: ACCEPT, REJECT, ALL
traffic_type = "REJECT"

# Enable VPC Flow Logs
enable_flow_logs = false

# Enable private subnets and associated resources
enable_private_subnets = false
